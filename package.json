{
  "name": "ahooks-x",
  "version": "1.0.0",
  "description": "ahooks-x is supplement for ahooks",
  "type": "module",
  "keywords": [
    "ahooks",
    "react hook",
    "ahooks-x"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hemengke1997/ahooks-x.git"
  },
  "author": "hemengke <https://github.com/hemengke1997>",
  "files": [
    "dist"
  ],
  "main": "./dist/lib/index.cjs",
  "module": "./dist/es/index.js",
  "types": "./dist/es/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/es/index.d.ts",
      "require": "./dist/lib/index.cjs",
      "import": "./dist/es/index.js",
      "default": "./dist/lib/index.cjs",
      "node": "./dist/lib/index.cjs"
    },
    "./use-url-state": {
      "types": "./dist/es/use-url-state/index.d.ts",
      "require": "./dist/lib/use-url-state/index.cjs",
      "import": "./dist/es/use-url-state/index.js",
      "default": "./dist/lib/use-url-state/index.cjs",
      "node": "./dist/lib/use-url-state/index.cjs"
    }
  },
  "sideEffects": false,
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "commitlint": "commitlint --edit",
    "prepare": "simple-git-hooks",
    "lint": "tsc --noEmit && eslint . --fix",
    "up": "taze -I -w",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "bump": "bumpp package.json -c --no-push -t --all -x \"npm run changelog\""
  },
  "peerDependencies": {
    "ahooks": ">=3.0.0",
    "query-string": ">=7.0.0",
    "react": ">=17.0.2",
    "react-dom": ">=17.0.2",
    "react-router-dom": ">=6.0.0"
  },
  "peerDependenciesMeta": {
    "react-router-dom": {
      "optional": true
    },
    "query-string": {
      "optional": true
    }
  },
  "dependencies": {
    "type-fest": "^4.26.0",
    "use-global-style": "^1.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.4.1",
    "@minko-fe/commitlint-config": "^2.1.2",
    "@minko-fe/eslint-config": "^4.0.0",
    "@minko-fe/prettier-config": "^2.2.3",
    "@minko-fe/tsconfig": "^2.1.1",
    "@types/react": "^18.3.4",
    "@types/react-dom": "^18.3.0",
    "bumpp": "^9.5.2",
    "conventional-changelog-cli": "^5.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^9.9.1",
    "npm-run-all": "^4.1.5",
    "simple-git-hooks": "^2.11.1",
    "taze": "^0.16.7",
    "tsup": "^8.2.4",
    "tsup-plugin-bundleless": "^0.4.0",
    "typescript": "^5.5.4"
  },
  "simple-git-hooks": {
    "commit-msg": "npm run commitlint",
    "pre-commit": "npm run lint"
  },
  "commitlint": {
    "extends": [
      "@minko-fe/commitlint-config"
    ]
  },
  "prettier": "@minko-fe/prettier-config"
}
